{
  "name": "nest-prisma-db-benchmark",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "db:gen": "prisma generate --schema ./libs/prisma-${SERVICE}/schema.prisma",
    "db:pull": "prisma db pull --schema ./libs/prisma-${SERVICE}/schema.prisma",
    "db:push": "prisma db push --schema ./libs/prisma-${SERVICE}/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema ./libs/prisma-${SERVICE}/schema.prisma",
    "db:migrate:dev": "prisma migrate dev --schema ./libs/prisma-${SERVICE}/schema.prisma --",
    "postinstall": "SERVICE=postgresql npm run db:gen && SERVICE=mongodb npm run db:gen"
  },
  "private": true,
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-fastify": "^9.2.1",
    "@opentelemetry/api": "^1.3.0",
    "@opentelemetry/auto-instrumentations-node": "^0.35.0",
    "@opentelemetry/exporter-jaeger": "^1.8.0",
    "@opentelemetry/exporter-prometheus": "^0.34.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.34.0",
    "@opentelemetry/instrumentation": "^0.34.0",
    "@opentelemetry/resources": "^1.8.0",
    "@opentelemetry/sdk-node": "^0.34.0",
    "@opentelemetry/sdk-trace-base": "^1.8.0",
    "@opentelemetry/semantic-conventions": "^1.8.0",
    "@opentelemetry/tracing": "^0.24.0",
    "@prisma/client": "^4.7.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "nestjs-otel": "^5.0.0",
    "prisma": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^9.2.2",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cli": "14.8.6",
    "@nrwl/eslint-plugin-nx": "14.8.6",
    "@nrwl/jest": "15.2.3",
    "@nrwl/linter": "14.8.6",
    "@nrwl/nest": "15.2.3",
    "@nrwl/node": "^15.2.3",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "14.8.6",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "nx": "14.8.6",
    "prettier": "^2.6.2",
    "prisma-erd-generator": "^1.2.4",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  }
